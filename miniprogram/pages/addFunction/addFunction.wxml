<view class="container">

  <view class="guide" class='positionForFixedBtn'>

    <button class='fixButton goodBtn'>扫描机身二维码</button>

    <text>以下机子等待检查：</text> 

    <block wx:for="{{deviceList}}">
      <van-cell
        title="{{item}}"
        size="large"
      />
    </block>
  
    <text>以下机子记录有误：</text> 

    <block wx:for="{{deviceNotFoundList}}">
  
      <van-cell
        title="{{item}}"
        size="large"
      />
    </block>

  </view>
      
  <button class = "fixButton goodBtn" bindtap="copyCode">Fixed</button>

</view>